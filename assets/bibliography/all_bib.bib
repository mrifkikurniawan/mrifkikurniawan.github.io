doi = {10.1186/s40486-017-0048-8},
file = {:home/user/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Ren, Jiang, Chae - 2017 - Effect of temperature on a miniaturized microbial fuel cell (MFC).pdf:pdf},
