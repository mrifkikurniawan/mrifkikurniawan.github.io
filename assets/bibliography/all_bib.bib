arxivId = {1505.03035},
author = {Luo, Zhi-Chao and Liu, Meng and Guo, Zhi-Nan and Jiang, Xiao-Fang and Luo, Ai-Ping and Zhao, Chu-Jun and Yu, Xue-Feng and Xu, Wen-Cheng and Zhang, Han},
