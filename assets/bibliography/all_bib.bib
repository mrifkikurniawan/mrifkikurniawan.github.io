keywords = {Industry 4.0,flexibility,human factor,smart assembly},
number = {1},
pages = {14958--14963},
publisher = {Elsevier B.V.},
title = {{Assembly system configuration through Industry 4.0 principles: the expected change in the actual paradigms}},
