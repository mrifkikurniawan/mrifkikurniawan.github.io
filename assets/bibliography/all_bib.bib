file = {:home/user/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Yu et al. - 2020 - Variable-Shot Adaptation for Online Meta-Learning.pdf:pdf},
keywords = {continual learning,meta-learning},
