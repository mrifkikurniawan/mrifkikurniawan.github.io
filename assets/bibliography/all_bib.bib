pages = {1--34},
publisher = {Association for Computing Machinery (ACM)},
title = {{Generalizing from a Few Examples: A Survey on Few-shot Learning}},
url = {https://dl.acm.org/doi/10.1145/3386252},
volume = {53},
year = {2020}
