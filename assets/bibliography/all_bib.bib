journal = {Journal of The Electrochemical Society},
number = {4},
pages = {F209--F216},
title = {{Impact of Hydrogen Bleeding into the Cathode Feed of a PEM Fuel Cell}},
url = {http://jes.ecsdl.org/lookup/doi/10.1149/2.0161704jes},
volume = {164},
year = {2017}
