keywords = {continual learning,rehearsal,replay},
mendeley-tags = {continual learning,rehearsal,replay},
title = {{Knowledge Capture and Replay for Continual Learning}},
url = {http://arxiv.org/abs/2012.06789},
year = {2020}
}
