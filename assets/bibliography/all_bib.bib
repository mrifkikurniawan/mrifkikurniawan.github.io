issn = {20452322},
journal = {Scientific Reports},
number = {1},
pages = {1--9},
publisher = {Springer US},
title = {{Strain softening of nano-scale fuzzy interfaces causes Mullins effect in thermoplastic polyurethane}},
url = {http://dx.doi.org/10.1038/s41598-017-00904-3},
volume = {7},
year = {2017}
}
@article{Vimala2014,
